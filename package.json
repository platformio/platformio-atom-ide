{
  "name": "platformio-ide",
  "main": "./lib/main",
  "version": "2.0.0-alpha.12",
  "description": "Official PlatformIO IDE for Atom. PlatformIO is an open source ecosystem for IoT development. Cross-platform build system and library manager. Continuous and IDE integration. Arduino and ARM mbed compatible.",
  "keywords": [
    "PlatformIO",
    "IoT",
    "embedded",
    "arduino",
    "mbed",
    "flash",
    "firmware",
    "esp8266",
    "esp32",
    "avr",
    "arm",
    "fpga",
    "verilog",
    "hardware",
    "serial",
    "monitor"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/platformio/platformio-atom-ide.git"
  },
  "bugs": {
    "url": "https://github.com/platformio/platformio-atom-ide/issues"
  },
  "license": "https://pioplus.com/license.html",
  "engines": {
    "atom": ">=1.12.2 <2.0.0"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "providedServices": {
    "builder": {
      "description": "PlatformIO IDE for Atom",
      "versions": {
        "2.0.0": "provideBuilder"
      }
    }
  },
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "<2.0.0": "consumeToolBar"
      }
    },
    "platformioIDETerminal": {
      "versions": {
        "^1.0.0": "consumePlatformioIDETerminal"
      }
    },
    "busy": {
      "versions": {
        "^1.0.0": "consumeBusy"
      }
    }
  },
  "dependencies": {
    "atom-select-list": "^0.1.0",
    "command-join": "^1.0.1",
    "flexboxgrid": "^6.3.1",
    "fs-extra": "^2.1.2",
    "fs-plus": "^3.0.0",
    "fuzzaldrin-plus": "^0.4.1",
    "getmac": "^1.2.1",
    "glob": "^7.1.1",
    "highlight.js": "^9.10.0",
    "humanize": "^0.0.9",
    "ini": "^1.3.4",
    "klaw": "^1.3.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "reactable": "^0.14.1",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.3",
    "request": "^2.81.0",
    "semver": "^5.3.0",
    "tar": "^2.2.1",
    "temp": "^0.8.3",
    "tmp": "^0.0.31"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.0",
    "esformatter": "^0.10.0",
    "esformatter-jsx": "^7.4.0",
    "eslint": "^3.18.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-redux-saga": "^0.3.0",
    "redux-logger": "^3.0.0"
  }
}
