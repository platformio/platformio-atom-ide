{
  "name": "platformio-ide",
  "main": "./lib/main",
  "version": "2.0.0-beta.7",
  "description": "Official PlatformIO IDE for Atom: The next generation integrated development environment for IoT. Cross-platform build system and unified debugger. Remote unit testing and firmware updates.",
  "keywords": [
    "PlatformIO",
    "IoT",
    "debugger",
    "unittest",
    "embedded",
    "arduino",
    "mbed",
    "flash",
    "firmware",
    "esp8266",
    "esp32",
    "avr",
    "arm",
    "fpga",
    "verilog",
    "hardware",
    "serial",
    "monitor"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/platformio/platformio-atom-ide.git"
  },
  "bugs": {
    "url": "https://github.com/platformio/platformio-atom-ide/issues"
  },
  "license": "Apache-2.0",
  "engines": {
    "atom": ">=1.13.0 <2.0.0"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "providedServices": {
    "builder": {
      "description": "PlatformIO IDE for Atom",
      "versions": {
        "2.0.0": "provideBuilder"
      }
    }
  },
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "<2.0.0": "consumeToolbar"
      }
    },
    "platformioIDETerminal": {
      "versions": {
        "^1.0.0": "consumePlatformioIDETerminal"
      }
    },
    "busy": {
      "versions": {
        "^1.0.0": "consumeBusy"
      }
    },
    "platformioIDEDebugger": {
      "versions": {
        "^1.0.0": "consumeDebugger"
      }
    }
  },
  "dependencies": {
    "atom-select-list": "^0.2.0",
    "command-join": "^1.1.1",
    "flexboxgrid": "^6.3.1",
    "fs-extra": "^2.1.2",
    "fs-plus": "^3.0.0",
    "fuzzaldrin-plus": "^0.4.1",
    "getmac": "^1.2.1",
    "glob": "^7.1.1",
    "highlight.js": "^9.11.0",
    "humanize": "^0.0.9",
    "ini": "^1.3.4",
    "klaw": "^2.0.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-simple-dropdown": "^3.0.0",
    "reactable": "^0.14.1",
    "redux": "^3.6.0",
    "redux-saga": "^0.15.3",
    "request": "^2.81.0",
    "semver": "^5.3.0",
    "tar": "3.1.8",
    "temp": "^0.8.3",
    "tmp": "^0.0.31"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.0",
    "esformatter": "^0.10.0",
    "esformatter-jsx": "^8.0.0",
    "eslint": "^4.1.1",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-redux-saga": "^0.4.0",
    "redux-logger": "^3.0.0"
  }
}
